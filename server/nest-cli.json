{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "nest-src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "tsconfig.json",
    "assets": [
      {
        "include": "grpc/protos/**/*.proto",
        "outDir": "dist/grpc/protos"
      }
    ]
  }
}
